@model IEnumerable<ASPFinal.Models.BOOK>

@{
    ViewBag.Title = "Book";
}
@Html.Partial("NavigationBox")

<div class="row">
    <div class="text-center">
        @if (Model != null)
        {
            foreach (var book in Model)
            {
                <img src='@Url.Content("~/Content/Images/@book_code.key/large.jpg")' />
                @*<img src="~/Content/Images/@book.BOOK_CODE/large.jpg" alt="Picture Missing" />*@
            }
        }
    </div>
</div>
